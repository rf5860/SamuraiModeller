<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<!-- Create Fragment -->
	<servlet>
		<servlet-name>CreateFragment</servlet-name>
		<servlet-class>samurai.CreateFragment</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateFragment</servlet-name>
		<url-pattern>/createFragment</url-pattern>
	</servlet-mapping>
<!-- Login User -->
	<servlet>
		<servlet-name>LoginUser</servlet-name>
		<servlet-class>samurai.LoginUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginUser</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
<!-- Logout User -->
	<servlet>
		<servlet-name>LogoutUser</servlet-name>
		<servlet-class>samurai.LogoutUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutUser</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
<!-- Create User -->
	<servlet>
		<servlet-name>CreateUser</servlet-name>
		<servlet-class>samurai.CreateUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateUser</servlet-name>
		<url-pattern>/createUser</url-pattern>
	</servlet-mapping>
<!-- Commandeer Fragment -->
	<servlet>
		<servlet-name>CommandeerFragment</servlet-name>
		<servlet-class>samurai.CommandeerFragment</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommandeerFragment</servlet-name>
		<url-pattern>/commandeer</url-pattern>
	</servlet-mapping>
<!--  Welcome File -->
	<welcome-file-list>
		<welcome-file>samurai.jsp</welcome-file>
	</welcome-file-list>
</web-app>
